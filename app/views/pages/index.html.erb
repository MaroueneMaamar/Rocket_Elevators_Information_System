								data-y="100" 
								data-speed="500"
								data-start="1500"
								data-easing="Cubic.easeOut"
								data-splitin="none"
								data-splitout="none"
								data-elementdelay="0.1"
								data-endelementdelay="0.1"
								data-endspeed="500"
								data-delay="1000"
								style="color:#fff; font-size:80px; line-height:130px; letter-spacing:0; text-shadow:none;">
									Fast, secure, beautiful. <br>
									<i>Rocket Elevators</i>
							</div>

							<!-- SERVICES BUTTON -->
							<div class="tp-caption sfb tp-resizeme"
								data-x="50"
								data-y="400" 
								data-speed="500"
								data-start="1500"
								data-easing="Power3.easeIn"
								data-splitin="none"
								data-splitout="none"
								data-elementdelay="1"
								data-endelementdelay="0.1"
								data-endspeed="500"
