					<ul class="hide">
					
				<!-- SLIDER 1  -->
						<li data-transition="parallaxtobottom" data-slotamount="7" data-masterspeed="600"  data-saveperformance="off"  data-title="Services">
							
						<%= image_tag("slider1.jpeg", alt: "cover image") %>
						
